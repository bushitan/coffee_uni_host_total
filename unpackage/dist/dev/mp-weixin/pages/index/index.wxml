<view class="container"><view class="cover"><image class="cover_image" src="{{theme.cover}}" mode="widthFix"></image></view><view class="title">{{theme.title}}</view><view class="summary">{{theme.summary}}</view><view><view style="height:5px;"></view><uni-list-item vue-id="1" show-arrow="{{false}}" title="{{theme.date}}" thumb="/static/location.png" bind:__l="__l"></uni-list-item><view style="height:2px;"></view><uni-list-item vue-id="2" show-arrow="{{true}}" title="{{theme.address}}" thumb="/static/location.png" data-event-opts="{{[['^click',[['openLoaction']]]]}}" bind:click="__e" bind:__l="__l"></uni-list-item></view><button class="btn" type="primary" data-event-opts="{{[['tap',[['sign',['$event']]]]]}}" bindtap="__e">报名</button><view style="height:100rpx;"></view></view>