<view class="content"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="node"><view class="explain">团队名称<view class="must">(必填)</view></view><view class="input_content"><input class="input" name="team_name" placeholder-style="input_pla" placeholder="请输入文工团名称" value="{{content.team_name}}"/></view></view><view class="node"><view class="explain">领队姓名<view class="must">(必填)</view></view><view class="input_content"><input class="input" name="leader_name" placeholder-style="input_pla" placeholder="请输入领队姓名" value="{{content.leader_name}}"/></view></view><view class="node"><view class="explain">领队联系电话<view class="must">(必填)</view></view><view class="input_content"><input class="input" name="leader_phone" placeholder-style="input_pla" placeholder="请输入领队联系电话" value="{{content.leader_phone}}"/></view></view><view class="node"><view class="explain">参赛曲目名称<view class="must">(必填)</view></view><view class="input_content"><input class="input" name="team_title" placeholder-style="input_pla" placeholder="请输入参赛曲目名称" value="{{content.title}}"/></view></view><view class="node"><view class="explain">其他信息<view class="must"></view></view><view class="input_content"><input class="input" name="other" placeholder-style="input_pla" value="" placeholder="请输入其他信息"/></view></view><view class="btn-area"><button class="btn" formType="submit">下一步</button></view></form></view>